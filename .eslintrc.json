{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-trailing-spaces":"off",
        "object-curly-spacing":"off",
        "padded-blocks":"off",
        "no-spaced-func":"off",
        "func-call-spacing":"off",
        "no-undef":"off",
        "no-shadow":"off",
        "lines-between-class-members":"off",
        "quotes":"off",
        "space-before-function-paren":"off",
        "no-multi-spaces":"off",
        "space-infix-ops":"off",
        "no-underscore-dangle":"off",
        "semi":"off",
        "no-console":"off",
        "no-unused-vars":"off",
        "eol-last":"off",
        "linebreak-style":"off",
        "indent":"off",
        "key-spacing":"off",
        "no-multiple-empty-lines":"off",
        "import/no-extraneous-dependencies":"off",
        "object-curly-newline":"off"
    }
}
